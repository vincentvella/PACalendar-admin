{
  "name": "light-bootstrap-dashboard-react",
  "version": "1.2.0",
  "private": true,
  "entry": "./main.js",
  "output": {
    "path": "__dirname",
    "filename": "bundle.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "chartist": "0.10.1",
